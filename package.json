{
  "name": "archei-companion-monorepo",
  "private": true,
  "packageManager": "pnpm@10.18.3",
  "scripts": {
    "dev:web": "pnpm --filter @archei/web dev",
    "build:web": "pnpm --filter @archei/web build",
    "dev:ws": "pnpm --filter @archei/realtime-server dev",
    "build:ws": "pnpm --filter @archei/realtime-server build",
    "start:ws": "pnpm --filter @archei/realtime-server start"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
